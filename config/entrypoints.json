{
  "endpoints": [
    {
      "url": "/api/users/login",
      "httpMethod": "post",
      "requiredPermission": "none",
      "function": "login"
    },
    {
      "url": "/api/users/signup",
      "httpMethod": "post",
      "requiredPermission": "none",
      "function": "signup"
    },
    {
      "url": "/api/users/info",
      "httpMethod": "get",
      "requiredPermission": "login",
      "function": "getUserInfo()"
    }
  ],
  "responses": {
    "getUserInfo": {
      "users[email:*email]": {
        "username": "username",
        "email": "email",
        "exercises": "exercise"
      }
    }
  }
}
